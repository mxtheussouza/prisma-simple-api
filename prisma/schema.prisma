generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}

model User {
    id          String      @id @default(uuid())
    name        String
    email       String      @unique
    password    String
    created_at  DateTime    @default(now())

    @@map("users")
}
